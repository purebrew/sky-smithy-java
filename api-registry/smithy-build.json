{
  "sources": ["./smithy"],
  "maven": {
    "dependencies": [
      "software.amazon.smithy:smithy-aws-traits:1.61.0",
      "software.amazon.smithy:smithy-openapi:1.61.0",
      "tech.purebrew.smithy:protocol:0.1.0-SNAPSHOT"
    ]
  },
  "projections": {
    "publicServices": {
      "transforms": [
        {
          "name": "includeServices",
          "args": {
            "services": ["com.example#CoffeeShop"]
          }
        },
        {
          "name": "excludeShapesByTrait",
          "args": { "traits": ["tech.purebrew.apigw#jwtClaim", "internal"] }
        },
        {
          "name": "inlinePublicServices"
        }
      ]
    }
  },
  "plugins": {
    "openapi": {
      "service": "com.example#CoffeeShop",
      "protocol": "aws.protocols#restJson1",
      "version": "3.1.0"
    }
  },
  "version": "1.0"
}
